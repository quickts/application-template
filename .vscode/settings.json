{
    "typescript.updateImportsOnFileMove.enabled": "always",
    "prettier.tabWidth": 4,
    "prettier.printWidth": 150,
    "prettier.endOfLine": "lf",
    "prettier.singleQuote": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "files.eol": "\n",
    "search.exclude": {
        "node_modules/": true,
        "dist/": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.meta": true
    }
}