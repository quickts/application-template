{
  "name": "@quickts/application-template",
  "version": "1.0.0",
  "description": "A quickts project",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:prod": "node dist/index.js",
    "test": "mocha -r tsconfig-paths/register -r ts-node/register --recursive -R spec test/*.ts"
  },
  "keywords": [],
  "author": "sbw",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.2",
    "mocha": "^6.1.4",
    "rimraf": "^2.6.3",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "tsconfig-paths": "^3.8.0"
  },
  "dependencies": {
    "@nestjs/common": "^6.2.4",
    "@nestjs/core": "^6.2.4",
    "@nestjs/platform-express": "^6.2.4",
    "@quickts/nestjs-auth": "^1.0.0",
    "@quickts/nestjs-cluster": "^1.0.3",
    "@quickts/nestjs-log4js": "^1.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quickts/application-template.git"
  },
  "bugs": "https://github.com/quickts/application-template/issues",
  "homepage": "https://github.com/quickts/application-template#readme"
}
